{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "baseBranches": ["main", "/^release-.*/"],
    "extends": [
        ":preserveSemverRanges",
        "group:allNonMajor",
    ],
    "lockFileMaintenance": {
        "commitMessageAction": "Update",
        "extends": [
            "group:allNonMajor"
        ]
    },
    "separateMajorMinor": false,
}
